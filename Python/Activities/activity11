fruits = {
    "apple": 120,
    "banana": 40,
    "mango": 100,
    "orange": 60,
    "grapes": 90
}
choice = input("Enter the fruit you want: ").lower()

if choice in fruits:
    print(f"Yes! {choice.capitalize()} is available for Rs.{fruits[choice]}/kg")
else:
    print(f"Sorry, {choice.capitalize()} is not available right now.")
